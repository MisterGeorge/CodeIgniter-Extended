msgid "The %s field is required."
msgstr "الحقل %s إجباري."

msgid "The %s field must have a value."
msgstr "الحقل %s يجب أن يحتوى على قيمة."

msgid "The %s field must contain a valid email address."
msgstr "الحقل %s يجب أن يحتوى عنوان بريد إلكتروني صحيح."

msgid "The %s field must contain all valid email addresses."
msgstr "الحقل %s يجب أن يحتوى على عناوين بريد إلكتروني صحيحة."

msgid "The %s field must contain a valid URL."
msgstr "الحقل %s يجب أن يحتوى على رابط."

msgid "The %s field must contain a valid IP."
msgstr "الحقل %s يجب أن يحتوى على عنوان أي بي."

msgid "The %s field must be at least %s characters in length."
msgstr "الحقل %s يجب أن لا يقل عن %s حرف."

msgid "The %s field cannot exceed %s characters in length."
msgstr "الحقل %s يجب أن لا يتجاوز أكثر من %s حرف."

msgid "The %s field must be exactly %s characters in length."
msgstr "الحقل %s يجب أن يكون بطول %s حرف."

msgid "The %s field may only contain alphabetical characters."
msgstr "الحقل %s يمكن أن يحتوى على أحرف فقط."

